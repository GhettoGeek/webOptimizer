{
    "bin": {"webOptimizer": "taskRunner.compiled.js"},
    "dependencies": {
        "babel-cli": "*",
        "babel-core": "*",
        "babel-loader": "*",
        "babel-plugin-transform-class-properties": "*",
        "babel-plugin-transform-decorators-legacy": "*",
        "babel-plugin-transform-flow-strip-types": "*",
        "babel-polyfill": "*",
        "babel-preset-es2015": "*",
        "coffee-loader": "*",
        "coffee-script": "*",
        "css-loader": "*",
        "eslint": "*",
        "extract-loader": "0.0.2",
        "extract-text-webpack-plugin": "^1.0.1",
        "file-loader": "*",
        "html-loader": "*",
        "html-webpack-plugin": "*",
        "image-webpack-loader": "^1.6.3",
        "imports-loader": "^0.6.5",
        "jade": "*",
        "jade-html-loader": "*",
        "jsdom": "*",
        "less": "*",
        "less-loader": "*",
        "node-sass": "*",
        "offline-plugin": "*",
        "rimraf": "*",
        "sass-loader": "*",
        "url-loader": "*",
        "webpack": "*",
        "webpack-dev-server": "*",
        "webpack-load-plugins": "*"
    },
    "description": "A generic web optimizer, (module) bundler and development environment.",
    "files": ["configurator.js", "webpackConfigurator.js", "taskRunner.js"],
    "homepage": "http://torben.website/webOptimizer",
    "keywords": ["webpack"],
    "license": "ISC",
    "main": "webpackConfiguration.compiled.js",
    "name": "webOptimizer",
    "repository": {
        "type" : "git", "url" : "https://github.com/thaibault/webOptimizer.git"
    },
    "scripts": {
        "preinstall": "babel taskRunner.js --presets es2015 --out-file taskRunner.compiled.js && babel webpackConfigurator.js --presets es2015 --out-file webpackConfigurator.compiled.js && babel configurator.js --presets es2015 --out-file configurator.compiled.js"
    },
    "version": "1.0.0",
    "configuration": {
        "default": {
            "debug": false,
            "sourcePath": "source/",
            "targetPath": "build/",
            "sourceAssetPath": {"__execute__": "`${self.sourcPath}asset/`"},
            "targetAssetPath": {"__execute__": "`${self.targetPath}asset/`"},
            "hashAlgorithm": "md5",
            "maximumInPlaceFileLimitInByte": 262144,
            "developmentTool": null,
            "internalInjects": [],
            "externalInjects": [],
            "inPlace": {"cascadingStyleSheet": true, "javaScript": false},
            "commandLineArguments": {
                "webpack": {
                    "__execute__": "`--config ${webOptimizerPath}/webpackConfigurator.compiled.js`"
                },
                "webpackDevServer": {
                    "__execute__": "`--config ${webOptimizerPath}/webpackConfigurator.compiled.js`"
                },
                "eslint": ""
            },
            "developmentServer": {
                "contentBase": {"__execute__": "self.sourcePath"},
                "historyApiFallback": true,
                "stats": {"colors": true}
            },
            "offline": {
                "caches": "all",
                "scope": "/",
                "updateStrategy": "hash",
                "version": 0,
                "ServiceWorker": {"output": "javaScript/serviceWorker.js"},
                "AppCache": {"directory": "/"},
                "excludes": ["`*.map?${self.hashAlgorithm}=*`"]
            },
            "files": {
                "html": [{
                    "debug": false,
                    "template": {
                        "__execute__": "`html?${JSON.stringify(self.html)}!jade-html?${JSON.stringify(self.jade)}!${self.sourcePath}index.jade`"
                    },
                    "hash": true,
                    "filename": "index.html",
                    "inject": "body",
                    "minify": {"collapseWhitespace": true}
                }],
                "cascadingStyleSheet": {"__execute__": "`cascadingStyleSheet/main.css?${self.hashAlgorithm}=[contenthash]`"},
                "javaScript": {"__execute__": "`javaScript/main.js?${self.hashAlgorithm}=[hash]`"}
            },
            "preprocessor": {
                "jade": {
                    "pretty": false,
                    "debug": false,
                    "includeManifest": {"__execute__": "!(self.offline === false || self.debug)"}
                },
                "sass": {"indentedSyntax": true},
                "scss": {},
                "modernJavaScript": {"cacheDirectory": true, "presets": ["es2015"]}
            },
            "html": {"attrs": "img:src link:href"},
            "cascadingStyleSheet": {},
            "optimizer": {
                "uglifyJS": {"compress": {"warnings": false}},
                "image": {
                    "file": {
                        "limit": {"__execute__": "self.maximumInPlaceFileLimitInByte"},
                        "name": {"__execute__": "`image/[name].[ext]?${self.hashAlgorithm}=[hash]`"
                    },
                    "content": {
                        "optimizationLevel": 7,
                        "bypassOnDebug": false,
                        "verbose": false
                    }
                },
                "font": {
                    "eot": {
                        "limit": {"__execute__": "self.maximumInPlaceFileLimitInByte"},
                        "name": {
                            "__execute__": "`font/[name].[ext]?${self.hashAlgorithm}=[hash]`"
                        }
                    },
                    "woff": {
                        "limit": {"__execute__": "self.maximumInPlaceFileLimitInByte"},
                        "name": {
                            "__execute__": "`font/[name].[ext]?${self.hashAlgorithm}=[hash]`"
                        }
                    },
                    "ttf": {
                        "limit": {"__execute__": "self.maximumInPlaceFileLimitInByte"},
                        "mimetype": "application/octet-stream",
                        "name": {
                            "__execute__": "`font/[name].[ext]?${self.hashAlgorithm}=[hash]`"
                        }
                    },
                    "svg": {
                        "limit": {"__execute__": "self.maximumInPlaceFileLimitInByte"},
                        "mimetype": "image/svg+xml",
                        "name": {
                            "__execute__": "`font/[name].[ext]?${self.hashAlgorithm}=[hash]`"
                        }
                    }
                }
            }
        },
        "debug": {
            "developmentTool": "#source-map",
            "offline": null,
            "files": {"html": [{"debug": true, "template": true}]},
            "preprocessor": {"jade": {"pretty": true, "debug": true}},
            "optimizer": {
                "uglifyJS": null,
                "image": {"content": {"bypassOnDebug": true, "verbose": true}}
            }
        }
    }
}
